{
    "interface": "enp0s3",
    "allow": {
    },
    "deny": {
        "l2_mac_addr_list": [
                "00:00:00:00:00:00",
                "ff:ff:ff:ff:ff:ff"
        ],
        "ethertypes": [
                "0x88dc"
        ],
        "vlan_ids": [
                1, 2
        ],
        "ipv4": {
            "protocol_except": 4,
            "flag_invalid_checksum": true,
            "flag_subnet_class_c_broadcast": true,
            "drop_senders": [
                "192.168.0.1",
                "192.168.0.2"
            ]
        }
    }
}

